ntrol input-md" /></td>\n' + '\t\t\t\t\t<td><button class="btn btn-success" ng-click="addField(newLabel, newName)"><i class="fa fa-plus"></i> Add</button></td>\n' + '\t\t\t\t</tr>\n' + '\t\t\t</tfooter>\n' + '\t\t</table>\n' + '\t</fieldset>\n' + '\t<div cn-field-options cn-data="editfieldOptions" cn-field="editfield" ng-show="editfield !== undefined" />\n' + '\t<fieldset>\n' + '\t\t<legend>Actions</legend>\n' + '\t\t<div>\n' + '\t\t\t<button class="btn btn-success" ng-click="save()"><i class="fa fa-floppy-o"></i> Save</button>\n' + '\t\t\t<button class="btn btn-danger" ng-click="remove()"><i class="fa fa-trash-o"></i> Delete</button>\n' + '\t\t</div>\n' + '\t</fieldset>\n' + '</form>');
